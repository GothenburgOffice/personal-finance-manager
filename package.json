{
  "name": "finance",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.1",
    "@fortawesome/free-brands-svg-icons": "^5.1.1",
    "@fortawesome/free-regular-svg-icons": "^5.1.1",
    "@fortawesome/free-solid-svg-icons": "^5.1.1",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "connected-react-router": "^4.3.0",
    "firebase": "^5.3.0",
    "firebaseui": "^3.1.1",
    "history": "^4.7.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-textfit": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules ./src/styles/scss/ -o ./src/styles/css/ --output-style expanded --source-map true",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules ./src/styles/scss/ -o ./src/styles/css/ --watch --recursive --output-style expanded --source-map true",
    "build-css-prod": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules ./src/styles/scss/ -o ./src/styles/css/ --output-style compressed -x",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css-prod build-js",
    "test": "react-scripts test --env=jsdom",
    "deploy": "run-s deploy-deploy deploy-login",
    "deploy-deploy": "firebase login",
    "deploy-login": "firebase deploy",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.1",
    "jest-enzyme": "^6.0.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.4.1",
    "redux-freeze": "^0.1.5",
    "redux-mock-store": "^1.5.3"
  }
}
